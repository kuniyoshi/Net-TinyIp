        foreach my $tag ( @tags ) {
            my $module = join q{::}, $class, map { ucfirst } split m{_}, $tag;
            eval "require $module"
                or die;
        }
    }

NAME
    Net::TinyIp - IP object

SYNOPSIS
      use Net::TinyIp;
      my $ip = Net::TinyIp->new( "192.168.1.1/24" );
      say $ip;

DESCRIPTION
    Net::TinyIp represents IP address.

